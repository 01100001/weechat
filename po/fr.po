msgstr "[serveur | pseudo]"

msgid ""
"  server: server name\n"
"nickname: nickname"
msgstr ""
"serveur: nom du serveur\n"
" pseudo: pseudo"

msgid "gives voice to nickname(s)"
msgstr "donne la voix à/aux pseudo(s)"

