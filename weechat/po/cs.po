#: src/irc/irc-commands.c:178
msgid "send notice message to user"
msgstr "poslat notifikační zprávu uživateli"

#: src/irc/irc-commands.c:179
msgid "nickname text"
msgstr "přezdívka text"

#: src/irc/irc-commands.c:180
